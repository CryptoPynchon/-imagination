<div class="gallery">
  <div class="row">
    <% @photographs.each do |p| %>
      <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
        <div class="thumbnail">
          <a href="<%= p.url %>" target="_blank">
            <%= image_tag "#{p.url}", width: "cover"; %>
              <div class="caption">
                  <%= "" %>
              </div>
          </a>
        </div>
      </div>
    <% end %>
  </div>
  <%= render 'components/footer-nav' %>
</div>
